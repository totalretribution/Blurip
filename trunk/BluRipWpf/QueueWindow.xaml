<Window x:Class="BluRip.QueueWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="{DynamicResource CaptionQueueWindow}" Height="400" Width="480" ResizeMode="CanResizeWithGrip" Icon="/BluRip;component/Blu-ray.ico" ShowInTaskbar="False" Closing="Window_Closing" LocationChanged="Window_LocationChanged" MinWidth="400" MinHeight="400">
    <Grid>
        <Label Content="{DynamicResource LabelQueueList}" Height="28" HorizontalAlignment="Left" Margin="12,12,0,0" Name="labelQueueList" VerticalAlignment="Top" />
        <ListBox HorizontalAlignment="Stretch" Margin="12,46,114,41" Name="listBox1" VerticalAlignment="Stretch" />
        <CheckBox Content="{DynamicResource CheckBoxQueueShutdown}" Height="16" HorizontalAlignment="Left" Margin="150,0,0,8" Name="checkBoxQueueShutdown" VerticalAlignment="Bottom" />
        <Button Content="{DynamicResource ButtonQueueStart}" Height="23" HorizontalAlignment="Left" Margin="12,0,0,8" Name="buttonQueueStart" VerticalAlignment="Bottom" Width="75" />
        <Button Content="{DynamicResource ButtonQueueUp}" Height="23" HorizontalAlignment="Right" Margin="0,46,12,0" Name="buttonQueueUp" VerticalAlignment="Top" Width="96" />
        <Button Content="{DynamicResource ButtonQueueDelete}" Height="23" HorizontalAlignment="Right" Margin="0,104,12,0" Name="buttonQueueDelete" VerticalAlignment="Top" Width="96" />
        <Button Content="{DynamicResource ButtonQueueDown}" Height="23" HorizontalAlignment="Right" Margin="0,75,12,0" Name="buttonQueueDown" VerticalAlignment="Top" Width="96" />
    </Grid>
</Window>
